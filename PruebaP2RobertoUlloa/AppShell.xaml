<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PruebaP2RobertoUlloa.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PruebaP2RobertoUlloa"
    Shell.FlyoutBehavior="Disabled"
    Title="PruebaP2RobertoUlloa">

    <ShellContent
        Title="Welcome"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
    Title="Pagina de Chistes"
    ContentTemplate="{DataTemplate local:ChistePage}"
    Route="ChistePage" />

    <ShellContent
    Title="Mi Informacion"
    ContentTemplate="{DataTemplate local:AboutPage}"
    Route="AboutPage" />
</Shell>
